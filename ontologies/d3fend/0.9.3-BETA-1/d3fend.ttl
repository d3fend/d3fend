d3f:TCGTrustedAttestationProtocolUseCasesForTPMFamilies1.2And2.0AndDICE a d3f:SpecificationReference,
        owl:NamedIndividual ;
    rdfs:label "TCG Trusted Attestation Protocol Use Cases for TPM Families 1.2 and 2.0 and DICE" ;
    d3f:has-link "https://trustedcomputinggroup.org/wp-content/uploads/TCG_TNC_TAP_Use_Cases_v1r0p35_published.pdf"^^xsd:anyURI ;
